// 测试 fibonacci 函数
function fibonacci(n) {
    println("fibonacci called with n = ")
    println(n)
    var a = 0
    var b = 1
    for (var i = 0; i < n; i = i + 1) {
        var temp = a
        a = b
        b = temp + b
    }
    println("Returning a = ")
    println(a)
    return a
}

println("Testing fibonacci(5):")
var result = fibonacci(5)
println("Final result:")
println(result)
